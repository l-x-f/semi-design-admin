{
  "name": "semi-design-admin",
  "private": true,
  "version": "0.0.1",
  "auth": "xiaofei",
  "description": "design by xiaofei",
  "license": "ISC",
  "copyright": "Copyright (c) 2022-present, xiaofei",
  "scripts": {
    "preinstall": "npx only-allow yarn",
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "build:prod": "vite build  --mode production",
    "report": "cross-env REPORT=true yarn build:prod",
    "tsc": "vue-tsc --noEmit --skipLibCheck",
    "serve": "vite preview",
    "prepare": "husky install",
    "commit": "git-cz",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx --fix",
    "stylelint": "stylelint  ./src/**/*.{css,less,sass,scss} --fix",
    "prettier": "prettier ./src/**/*.{js,jsx,ts,tsx,css,less,sass,scss} --write"
  },
  "dependencies": {
    "@ahooksjs/use-url-state": "^3.5.0",
    "@douyinfe/semi-ui": "^2.17.0",
    "@semi-bot/semi-theme-volcano_engine": "^1.0.13",
    "accounting": "^0.4.1",
    "ahooks": "^3.4.1",
    "autoprefixer": "^10.4.7",
    "axios": "^0.27.2",
    "date-fns": "^2.28.0",
    "dayjs": "^1.11.3",
    "echarts": "^5.3.3",
    "file-saver": "^2.0.5",
    "js-cookie": "^3.0.1",
    "js-md5": "^0.7.3",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "numeral": "^2.0.6",
    "postcss": "^8.4.14",
    "react": "^18.1.0",
    "react-dnd": "14",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0",
    "recoil": "^0.7.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@types/accounting": "^0.4.2",
    "@types/file-saver": "^2.0.5",
    "@types/js-cookie": "^3.0.2",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.42",
    "@types/nprogress": "^0.2.0",
    "@types/numeral": "^2.0.2",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "@vitejs/plugin-legacy": "^1.8.2",
    "@vitejs/plugin-react": "^1.3.0",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.0.1",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.1",
    "mockjs": "^1.1.0",
    "prettier": "^2.6.2",
    "rollup-plugin-visualizer": "^5.6.0",
    "sass": "^1.52.3",
    "stylelint": "^14.9.1",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.0.1",
    "typescript": "^4.6.3",
    "vite": "^3.0.7",
    "vite-plugin-banner": "^0.3.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-svg-icons": "^2.0.1"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,vue}": [
      "npm run lint"
    ],
    "src/**/*.{vue,css,less,sass,scss}": [
      "npm run stylelint"
    ]
  },
  "resolutions": {
    "bin-wrapper": "npm:bin-wrapper-china"
  },
  "engines": {
    "node": ">=12",
    "yarn": "<2"
  }
}
